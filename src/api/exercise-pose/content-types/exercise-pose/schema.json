{
  "kind": "collectionType",
  "collectionName": "exercise_poses",
  "info": {
    "singularName": "exercise-pose",
    "pluralName": "exercise-poses",
    "displayName": "ExercisePose"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "blocks"
    },
    "calories": {
      "type": "integer"
    },
    "duration": {
      "default": 0,
      "type": "decimal"
    },
    "daily_exercises": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::daily-exercise.daily-exercise",
      "mappedBy": "exercise"
    }
  }
}
