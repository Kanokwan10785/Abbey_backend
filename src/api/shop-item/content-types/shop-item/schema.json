{
  "kind": "collectionType",
  "collectionName": "shop_items",
  "info": {
    "singularName": "shop-item",
    "pluralName": "shop-items",
    "displayName": "Shop-item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Shirt-item",
        "Pant-item",
        "Skin-item",
        "Food-item"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "label": {
      "type": "string"
    },
    "isSinglePurchase": {
      "type": "boolean"
    },
    "clothing_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::clothing-item.clothing-item",
      "mappedBy": "choose_clothes"
    },
    "pet_food_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::pet-food-item.pet-food-item",
      "mappedBy": "choose_foot"
    }
  }
}
